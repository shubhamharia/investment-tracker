# Example environment file for Investment Tracker
# Copy to .env and fill values before deploying to production.

# PostgreSQL
POSTGRES_DB=investment_tracker
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change-me-please
DATABASE_URL=postgresql://postgres:change-me-please@db:5432/investment_tracker

# Redis
REDIS_URL=redis://redis:6379/0

# Flask settings
FLASK_ENV=production
SECRET_KEY=please-change-this-to-a-secure-value
JWT_SECRET_KEY=please-change-this-to-a-secure-value

# CSV import
AUTO_IMPORT_CSV=false
CSV_FILE_PATH=/app/data/combined_transactions_updated.csv

# Logging
LOG_LEVEL=INFO
